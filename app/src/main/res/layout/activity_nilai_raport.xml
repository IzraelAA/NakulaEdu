<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@drawable/backgroundchat"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".NilaiRaportActivity">
    <androidx.appcompat.widget.Toolbar
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:title="Nilai Raport"
        android:background="@drawable/gradianttoolbar"
        app:titleTextColor="#FFF"/>
    <androidx.recyclerview.widget.RecyclerView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:listitem="@layout/nilairaportlist"
        android:id="@+id/reclerviewnilairaport"/>
</LinearLayout>